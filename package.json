{
	"name": "satori-factory",
	"version": "0.0.0",
	"description": "Tiny yet full-fledged JSX factory for Satori",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./jsx-runtime": {
			"import": "./dist/jsx-runtime.js",
			"types": "./dist/jsx-runtime.d.ts"
		},
		"./jsx-dev-runtime": {
			"import": "./dist/jsx-dev-runtime.js",
			"types": "./dist/jsx-dev-runtime.d.ts"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"lint": "biome check .",
		"test": "vitest",
		"prepack": "npm run build"
	},
	"keywords": ["jsx", "satori"],
	"author": "Yudai Nakata",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.6.1",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3",
		"vitest": "^1.2.2"
	}
}
